}

/* ----- Buttons ----- */
QPushButton {
    background-color: #323c41;
    color: #d3c6aa;
    border: 1px solid #4c555b;
    border-radius: 4px;
    padding: 5px 10px;
}

QPushButton:hover {
    background-color: #374247;
    border: 1px solid #a7c080;
}

QPushButton:pressed {
    background-color: #a7c080;
    color: #2b3339;
}

/* ----- LineEdits / TextEdits ----- */
QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: #232a2e;
    border: 1px solid #4c555b;
    padding: 4px;
    color: #d3c6aa;
    selection-background-color: #7fbbb3;
    selection-color: #2b3339;
}

QLineEdit:hover, QTextEdit:hover, QPlainTextEdit:hover {
    border: 1px solid #a7c080;
}

/* ----- Labels ----- */
QLabel {
    color: #d3c6aa;
}

/* ----- ComboBox ----- */
QComboBox {
    background-color: #323c41;
    border: 1px solid #4c555b;
    padding: 4px;
    color: #d3c6aa;
}

QComboBox:hover {
    border: 1px solid #a7c080;
}

QComboBox QAbstractItemView {
    background-color: #2b3339;
    selection-background-color: #7fbbb3;
    selection-color: #2b3339;
}

/* ----- Checkboxes &amp; Radio Buttons ----- */
QCheckBox, QRadioButton {
    spacing: 6px;
    color: #d3c6aa;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #4c555b;
    background: #232a2e;
}

QCheckBox::indicator:hover, QRadioButton::indicator:hover {
    border: 1px solid #a7c080;
}

QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background: #a7c080;
    border: 1px solid #a7c080;
}

/* ----- Progress Bar ----- */
QProgressBar {
    border: 1px solid #4c555b;
    text-align: center;
    background-color: #232a2e;
    color: #d3c6aa;
}

QProgressBar::chunk {
    background-color: #a7c080;
}

/* ----- Scrollbars ----- */
QScrollBar:vertical {
    border: none;
    background: #232a2e;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #4c555b;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: #a7c080;
}

/* ----- Slider ----- */
QSlider::groove:horizontal {
    border: 1px solid #4c555b;
    height: 8px;
    background: #232a2e;
}

QSlider::handle:horizontal {
    background: #4c555b;
    border: 1px solid #4c555b;
    width: 16px;
    margin: -4px 0;
    border-radius: 8px;
}

QSlider::groove:horizontal:hover, QSlider::handle:horizontal:hover {
    border: 1px solid #a7c080;
}

QSlider::sub-page:horizontal {
    background: #a7c080;
    border-radius: 4px;
}

/* ----- Menus ----- */
QMenuBar {
    background-color: #323c41;
    color: #d3c6aa;
}

QMenuBar::item:selected {
    background: #7fbbb3;
    color: #2b3339;
}

QMenu {
    background-color: #2b3339;
    border: 1px solid #4c555b;
    color: #d3c6aa;
}

QMenu::item:selected {
    background-color: #7fbbb3;
    color: #2b3339;
}</string>
